{
  "env": {
      "browser": true,
      "es2022": true
  },
  "extends": ["prettier", "eslint:recommended"],
  "plugins": ["prettier"],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
  },
  "rules": {
      "prettier/prettier": "error"
  }
}
